  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 6.9.1"
  },
  "scripts": {
    "test": "npm run jshint && npm run ava",
    "jshint": "jshint src test --config",
    "start": "node dist/",
    "start:dev": "DEBUG=app* babel-node src/",
