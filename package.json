    "feathers": "2.0.2",
    "feathers-configuration": "0.3.3",
    "feathers-errors": "2.4.0",
    "feathers-hooks": "1.6.1",
    "feathers-mongoose": "3.5.3",
    "feathers-rest": "1.5.1",
    "lodash": "4.16.6",
    "mongoose": "4.6.5",
    "mongoose-hidden": "0.6.4",
    "serve-favicon": "2.3.0",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.16.0",
